R Codes used for the project:

#Data input for each state
Arkansas <- read.table(file.choose(), header = TRUE, sep = ",")
California <- read.table(file.choose(), header = TRUE, sep = ",")
DC <- read.table(file.choose(), header = TRUE, sep = ",")
Indiana <- read.table(file.choose(), header = TRUE, sep = ",")
NewYork <- read.table(file.choose(), header = TRUE, sep = ",")
Wyoming <- read.table(file.choose(), header = TRUE, sep = ",")

#Pairs Plot for each state
pairs(Arkansas, main = "Arkansas")
pairs(California, main = "California")
pairs(DC, main = "DC")
pairs(Indiana, main = "Indiana")
pairs(NewYork, main = "New York")
pairs(Wyoming, main = "Wyoming")

#Adjusted data for each state
Arkansas15 <- read.table(file.choose(), header = TRUE, sep = ",")
California15 <- read.table(file.choose(), header = TRUE, sep = ",")
DC15 <- read.table(file.choose(), header = TRUE, sep = ",")
Indiana15 <- read.table(file.choose(), header = TRUE, sep = ",")
NewYork15 <- read.table(file.choose(), header = TRUE, sep = ",")
Wyoming15 <- read.table(file.choose(), header = TRUE, sep = ",")

#Pairs Plot for each state with adjusted data
pairs(Arkansas15, main = "Arkansas Adj Abortion")
pairs(California15, main = "California Adj Abortion")
pairs(DC15, main = "DC Adj Abortion")
pairs(Indiana15, main = "Indiana Adj Abortion")
pairs(NewYork15, main = "New York Adj Abortion")
pairs(Wyoming15, main = "Wyoming Adj Abortion")

#Correlation Plot for each state
corArkansas <- as.matrix(Arkansas[,c(3:10)])
corCalifornia <- as.matrix(California[,c(3:10)])
corDC <- as.matrix(DC[,c(3:10)])
corIndiana <- as.matrix(Indiana[,c(3:10)])
corNewYork <- as.matrix(NewYork[,c(3:10)])
corWyoming <- as.matrix(Wyoming[,c(3:10)])

corrplot(cor(corCalifornia), main = "California", mar = c(1,1,1.5,1), tl.cex = .5)
corrplot(cor(corArkansas), main = "Arkansas", mar = c(1,1,1.5,1), tl.cex = .5)
corrplot(cor(corDC), main = "DC", mar = c(1,1,1.5,1), tl.cex = .5)
corrplot(cor(corIndiana), main = "Indiana", mar = c(1,1,1.5,1), tl.cex = .5)
corrplot(cor(corNewYork), main = "New York", mar = c(1,1,1.5,1), tl.cex = .5)
corrplot(cor(corWyoming), main = "Wyoming", mar = c(1,1,1.5,1), tl.cex = .5)

corArkansas15 <- as.matrix(Arkansas15[,c(3:10)])
corCalifornia15 <- as.matrix(California15[,c(3:10)])
corDC15 <- as.matrix(DC15[,c(3:10)])
corIndiana15 <- as.matrix(Indiana15[,c(3:10)])
corNewYork15 <- as.matrix(NewYork15[,c(3:10)])
corWyoming15 <- as.matrix(Wyoming15[,c(3:10)])

corrplot(cor(corCalifornia15), main = "California Adj Abortion", mar = c(1,1,1.5,1), tl.cex = .5)
corrplot(cor(corArkansas15), main = "Arkansas Adj Abortion", mar = c(1,1,1.5,1), tl.cex = .5)
corrplot(cor(corDC15), main = "DC Adj Abortion", mar = c(1,1,1.5,1), tl.cex = .5)
corrplot(cor(corIndiana15), main = "Indiana Adj Abortion", mar = c(1,1,1.5,1), tl.cex = .5)
corrplot(cor(corNewYork15), main = "New York Adj Abortion", mar = c(1,1,1.5,1), tl.cex = .5)
corrplot(cor(corWyoming15), main = "Wyoming Adj Abortion", mar = c(1,1,1.5,1), tl.cex = .5)

#Arkansas Violent Crime ANOVA
Arkansasvcrime1 <- lm(Arkansas$VIOLENT.CRIME.RATE ~ Arkansas$UNEMPLOYMENT.RATE)
Arkansasvcrime2 <- lm(Arkansas$VIOLENT.CRIME.RATE ~ Arkansas$UNEMPLOYMENT.RATE + Arkansas$PER.CAPITA.INCOME)
Arkansasvcrime3 <- lm(Arkansas$VIOLENT.CRIME.RATE ~ Arkansas$UNEMPLOYMENT.RATE + Arkansas$PER.CAPITA.INCOME + Arkansas$ABORTION.RATE)
anova(Arkansasvcrime1)

